{
    "name": "miniature-office/php-floor",
    "description": "Floor 15 - PHP Jurisdiction for Miniature Office",
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "classmap": [
            "department_floor.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs --standard=PSR12 .",
        "analyze": "phpstan analyze -l max .",
        "fix": "phpcbf --standard=PSR12 ."
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
